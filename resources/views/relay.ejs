<!doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Browser ID</title>
</head>
  <body>
      Relay iframe.  Woohoo!
      <% if (production) { %>
        <script type='text/javascript'
          src='/production/relay.js'></script>

      <% } else { %>
        <script type='text/javascript'
          src='/lib/jschannel.js'></script>
        <script type='text/javascript'
          src='/shared/browserid.js'></script>
        <script type='text/javascript'
          src='/relay/relay.js'></script>
      <% } %>

      <script type="text/javascript">
        var relay = BrowserID.Relay;
        relay.init({
          channel: Channel,
          window: window.parent
        });

        relay.open();
      </script>
	</body>
</html>
